generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement()) @unique
  uid      String  @unique
  role     String  @default("User")
  name     String
  email    String  
  password String?
  image    String  @default("https://dummyimage.com/400x400")
  provider String
}
